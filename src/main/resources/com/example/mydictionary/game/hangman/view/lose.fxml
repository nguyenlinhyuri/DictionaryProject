<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mydictionary.game.hangman.LoseHangman">
    <children>
      <ImageView fitHeight="687.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/bg.png" />
         </image>
      </ImageView>
        <ImageView fx:id="myImage" fitHeight="15.0" fitWidth="67.0" layoutX="443.0" layoutY="245.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../image/lose.png" />
            </image>
        </ImageView>
        <Button fx:id="QuitButton" layoutX="226.0" layoutY="430.0" mnemonicParsing="false" onAction="#quitgame" prefHeight="48.0" prefWidth="126.0" style="-fx-background-radius: 20; -fx-font-size: 20; -fx-font-family: cambria; -fx-background-color: white;" stylesheets="@../../../css/style.css" text="Quit" />
        <Button fx:id="rePlayButton" layoutX="572.0" layoutY="430.0" mnemonicParsing="false" onAction="#replay" prefHeight="48.0" prefWidth="126.0" style="-fx-background-radius: 20; -fx-background-color: white; -fx-font-family: cambria; -fx-font-size: 20;" stylesheets="@../../../css/style.css" text="Replay">
            <font>
                <Font name="System Bold Italic" size="12.0" />
            </font></Button>
        <Label fx:id="tagetLB" alignment="CENTER" layoutX="288.0" layoutY="53.0" prefHeight="48.0" prefWidth="324.0" style="-fx-font-family: cambria; -fx-font-size: 32;" text="target">
            <font>
                <Font name="System Bold" size="22.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
