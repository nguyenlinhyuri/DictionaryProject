<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mydictionary.game.hangman.CongratulationHangman">
    <children>
      <ImageView fitHeight="648.0" fitWidth="900.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/bg.png" />
         </image>
      </ImageView>
        <Button fx:id="nextButon" layoutX="136.0" layoutY="407.0" mnemonicParsing="false" onAction="#next" prefHeight="46.0" prefWidth="127.0" style="-fx-background-radius: 20; -fx-font-family: cambria; -fx-background-color: white; -fx-font-size: 20;" stylesheets="@../../../css/style.css" text="Next Word" textFill="#000000ed">
            <font>
                <Font size="20.0" />
            </font></Button>
        <ImageView fx:id="myImage" fitHeight="233.0" fitWidth="239.0" layoutX="616.0" layoutY="197.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../image/clap.gif" />
            </image>
        </ImageView>
      <Button fx:id="quitButton" layoutX="329.0" layoutY="406.0" mnemonicParsing="false" onAction="#quitAction" prefHeight="46.0" prefWidth="203.0" style="-fx-background-radius: 20; -fx-font-family: cambria; -fx-background-color: white; -fx-font-size: 20;" stylesheets="@../../../css/style.css" text="Play More Games" textFill="#000000ed">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label fx:id="tagetLB" alignment="CENTER" layoutX="122.0" layoutY="171.0" prefHeight="48.0" prefWidth="395.0" style="-fx-font-family: cambria; -fx-font-size: 32; -fx-font-weight: bold;" text="Congratulation!">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="wordLabel" alignment="CENTER" layoutX="122.0" layoutY="252.0" prefHeight="48.0" prefWidth="395.0" style="-fx-font-family: cambria; -fx-font-size: 30;" text="The word is ">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
