      <Button fx:id="flashcardsButton" layoutX="70.0" layoutY="210.0" mnemonicParsing="false" onAction="#flashcardsAction" prefHeight="100.0" prefWidth="670.0" style="-fx-font-family: Cambria; -fx-background-radius: 20; -fx-font-size: 20; -fx-background-color: #FDCAC9; -fx-font-weight: bold; -fx-cursor: hand;" stylesheets="@../css/style.css" text=" Flashcards                                                                                                        ">
         <font>
            <Font size="18.0" />
