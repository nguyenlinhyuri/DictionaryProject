         <font>
            <Font size="18.0" />
         </font>
         <graphic>
            <ImageView fitHeight="72.0" fitWidth="69.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../practice/image/noted.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="exercisesButton" layoutX="70.0" layoutY="320.0" mnemonicParsing="false" onAction="#exercisesAction" prefHeight="100.0" prefWidth="670.0" style="-fx-font-family: Cambria; -fx-background-radius: 20; -fx-font-size: 20; -fx-font-weight: bold; -fx-background-color: #EEF1E6; -fx-cursor: hand;" stylesheets="@../css/style.css" text="    Exercises                                                                                                           ">
