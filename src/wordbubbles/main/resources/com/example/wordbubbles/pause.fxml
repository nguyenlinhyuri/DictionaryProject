<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="520.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.wordbubbles.PauseController">
   <children>
      <Label alignment="CENTER" prefHeight="51.0" prefWidth="128.0" text="PAUSE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="restartButton" layoutX="287.0" layoutY="231.0" mnemonicParsing="false" onAction="#restartGame" prefHeight="58.0" prefWidth="225.0" text="Restart">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="quitButton" layoutX="287.0" layoutY="289.0" mnemonicParsing="false" onAction="#quitGame" prefHeight="58.0" prefWidth="225.0" text="Quit">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="resumeButton" layoutX="287.0" layoutY="172.0" mnemonicParsing="false" onAction="#resumeGame" prefHeight="58.0" prefWidth="225.0" text="Resume">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
