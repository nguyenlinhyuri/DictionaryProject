<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="520.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.wordsnatcher.EndController">
    <children>
        <Label alignment="CENTER" layoutX="284.0" layoutY="103.0" prefHeight="62.0" prefWidth="232.0" text="Congratulation!">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
        <Label fx:id="endScoreLabel" alignment="CENTER" layoutX="317.0" layoutY="165.0" prefHeight="62.0" prefWidth="166.0" text="Score: ">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Button fx:id="replayButton" layoutX="170.0" layoutY="342.0" mnemonicParsing="false" onAction="#replayGame" prefHeight="37.0" prefWidth="98.0" text="Play Again">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Button fx:id="playMoreGameButton" layoutX="275.0" layoutY="342.0" mnemonicParsing="false" onAction="#playMoreGame" prefHeight="37.0" prefWidth="417.0" text="Play More Games">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
